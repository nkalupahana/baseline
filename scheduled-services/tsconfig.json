{
    "compilerOptions": {
        "module": "Node16",
        "moduleResolution": "Node16",
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "outDir": "lib",
        "sourceMap": true,
        "strict": true,
        "target": "es2017",
        "skipLibCheck": true,
        "esModuleInterop": true
    },
    "compileOnSave": true,
    "include": ["src"]
}
