{
	"appId": "app.getbaseline.baseline",
	"appName": "baseline",
	"webDir": "build",
	"bundledWebRuntime": false,
	"ios": {
		"scheme": "baseline"
	},
	"plugins": {
		"FirebaseAuthentication": {
			"skipNativeAuth": true,
			"providers": [
				"apple.com",
				"google.com"
			]
		},
		"Keyboard": {
			"resize": "none"
		}
	},
	"backgroundColor": {
		"light": "#FFFFFF",
		"dark": "#080808"
	}
}
