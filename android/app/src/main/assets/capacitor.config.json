{
	"appId": "app.getbaseline.baseline",
	"appName": "baseline",
	"webDir": "build",
	"ios": {
		"scheme": "baseline"
	},
	"plugins": {
		"FirebaseAuthentication": {
			"skipNativeAuth": true,
			"providers": [
				"apple.com",
				"google.com"
			]
		},
		"Keyboard": {
			"resize": "none"
		},
		"LocalNotifications": {
			"largeIcon": "notification_logo",
			"smallIcon": "notification_icon",
			"iconColor": "#03A9F4"
		}
	}
}
